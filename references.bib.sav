@Article{Voss_1984,
  author    = {R F Voss},
  title     = {The fractal dimension of percolation cluster hulls},
  journal   = {Journal of Physics A: Mathematical and General},
  year      = {1984},
  volume    = {17},
  number    = {7},
  pages     = {L373--L377},
  month     = {may},
  abstract  = {Large simulations of 2D site percolation on a square lattice provide several estimates of the fractal dimension Dh of the 'hull' or outer boundary of percolation clusters close to criticality. Statistical estimates based on an ensemble of more than 106 clusters with varying sizes yield Dh=1.74+or-0.02. Geometrical measurements of the hull two-point correlation for individual clusters with more than 105 sites give Dh=1.76+or-0.01. The observed scaling behaviour is the same both slightly above and below pc.},
  doi       = {10.1088/0305-4470/17/7/001},
  publisher = {{IOP} Publishing},
  url       = {https://doi.org/10.1088%2F0305-4470%2F17%2F7%2F001},
}

