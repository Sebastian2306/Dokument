@Article{Kammerer_2008,
  author    = {Axel Kammerer and Felix Höfling and Thomas Franosch},
  title     = {Cluster-resolved dynamic scaling theory and universal corrections for transport on percolating systems},
  journal   = {{EPL} (Europhysics Letters)},
  year      = {2008},
  volume    = {84},
  number    = {6},
  pages     = {66002},
  month     = {dec},
  abstract  = {For percolating systems, we propose a universal exponent relation connecting the leading corrections to scaling of the cluster size distribution with the dynamic corrections to the asymptotic transport behaviour at criticality. Our derivation is based on a cluster-resolved scaling theory unifying the scaling of both the cluster size distribution and the dynamics of a random walker. We corroborate our theoretical approach by extensive simulations for a site percolating square lattice and numerically determine both the static and dynamic correction exponents.},
  doi       = {10.1209/0295-5075/84/66002},
  publisher = {{IOP} Publishing},
  url       = {https://doi.org/10.1209%2F0295-5075%2F84%2F66002},
}

